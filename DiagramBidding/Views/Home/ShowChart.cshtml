@{

    var myChart = new Chart(width: 1000, height: 370) 
        .AddTitle("Курсы валют")
        .AddLegend("Валюта");

    foreach (var item in ViewBag.currency)
    {

        myChart.AddSeries(
        name: item.currency,
        chartType: "Line",
        xValue: item.date,
        yValues: item.price);

    }
    myChart.Write();
}
